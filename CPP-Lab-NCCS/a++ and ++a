#include <iostream>

using namespace std;
class op{
  int x=5;
  
  public:
  
  // for x++
  void operator ++(int){
      
      x++;
      
  }
  
  // for ++x
    void operator ++(){
      
      x++;
      
  }
  
  
  void display(){
      cout<<x;
  }
    
};
int main()
{
    
    op o;
   
    
    // ++o;
    ++o;
    //o++
    
    // o++;
    
    
    o.display();
   

    return 0;
}
